entry_points:
  main:
    parameters:
      split: {type: string, default: test}
      prefix: string
      sobel: {type: float, default: 1}
      max: {type: float, default: 0}
    command: "python3 script.py -s {split} -p {prefix} --sobel {sobel} --max {max}"